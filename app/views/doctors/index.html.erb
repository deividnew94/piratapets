<p id="notice"><%= notice %></p>

<h1>Doctors</h1>

<table>
  <thead>
    <tr>
      <th>Iddoctor</th>
      <th>Nombre</th>
      <th>Apellido paterno</th>
      <th>Apellido materno</th>
      <th>Email</th>
      <th>Telefono</th>
      <th>Administrador idadministrador</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @doctors.each do |doctor| %>
      <tr>
        <td><%= doctor.IdDoctor %></td>
        <td><%= doctor.Nombre %></td>
        <td><%= doctor.Apellido_Paterno %></td>
        <td><%= doctor.Apellido_Materno %></td>
        <td><%= doctor.Email %></td>
        <td><%= doctor.Telefono %></td>
        <td><%= doctor.ADMINISTRADOR_IdAdministrador %></td>
        <td><%= link_to 'Show', doctor %></td>
        <td><%= link_to 'Edit', edit_doctor_path(doctor) %></td>
        <td><%= link_to 'Destroy', doctor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Doctor', new_doctor_path %>
