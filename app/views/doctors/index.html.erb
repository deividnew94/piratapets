</br></br><div align="center"><h1>LISTA DE DOCTORES VETERINARIOS</h1></div>
</br></br></br>
<table class="table table-bordered">
  <thead>
    <tr>
      <th>ID DOCTOR</th>
      <th>NOMBRE</th>
      <th>APELLIDO PATERNO</th>
      <th>APELLIDO MATERNO</th>
      <th>E-MAIL</th>
      <th>TELEFONO</th>
      <th>ADMINISTRADOR ID</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @doctors.each do |doctor| %>
      <tr>
        <td><%= doctor.IdDoctor %></td>
        <td><%= doctor.Nombre %></td>
        <td><%= doctor.Apellido_Paterno %></td>
        <td><%= doctor.Apellido_Materno %></td>
        <td><%= doctor.Email %></td>
        <td><%= doctor.Telefono %></td>
        <td><%= doctor.ADMINISTRADOR_IdAdministrador %></td>
        <td><%= link_to 'Show', doctor %></td>
        <td><%= link_to 'Edit', edit_doctor_path(doctor) %></td>
        <td><%= link_to 'Destroy', doctor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

