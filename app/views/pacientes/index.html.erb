<p id="notice"><%= notice %></p>

<h1>Pacientes</h1>

<table>
  <thead>
    <tr>
      <th>Idpacientes</th>
      <th>Nombre duenio</th>
      <th>Especie</th>
      <th>Raza</th>
      <th>Genero</th>
      <th>Tipo sangre</th>
      <th>Tamanio</th>
      <th>Actividad</th>
      <th>Peso</th>
      <th>Fecha nacimiento</th>
      <th>Codigo paciente</th>
      <th>Clientes idcliente</th>
      <th>Clientes administrador idadministrador</th>
      <th>Historia idhistoria</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pacientes.each do |paciente| %>
      <tr>
        <td><%= paciente.IdPacientes %></td>
        <td><%= paciente.Nombre_Duenio %></td>
        <td><%= paciente.Especie %></td>
        <td><%= paciente.Raza %></td>
        <td><%= paciente.Genero %></td>
        <td><%= paciente.Tipo_Sangre %></td>
        <td><%= paciente.Tamanio %></td>
        <td><%= paciente.Actividad %></td>
        <td><%= paciente.Peso %></td>
        <td><%= paciente.Fecha_Nacimiento %></td>
        <td><%= paciente.Codigo_Paciente %></td>
        <td><%= paciente.CLIENTES_IdCliente %></td>
        <td><%= paciente.CLIENTES_ADMINISTRADOR_IdAdministrador %></td>
        <td><%= paciente.HISTORIA_IdHistoria %></td>
        <td><%= link_to 'Show', paciente %></td>
        <td><%= link_to 'Edit', edit_paciente_path(paciente) %></td>
        <td><%= link_to 'Destroy', paciente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Paciente', new_paciente_path %>
