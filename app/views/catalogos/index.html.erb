<p id="notice"><%= notice %></p>

<h1>Catalogos</h1>

<table>
  <thead>
    <tr>
      <th>Idcatalogo</th>
      <th>Productos</th>
      <th>Servicios</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @catalogos.each do |catalogo| %>
      <tr>
        <td><%= catalogo.IdCatalogo %></td>
        <td><%= catalogo.Productos %></td>
        <td><%= catalogo.Servicios %></td>
        <td><%= link_to 'Show', catalogo %></td>
        <td><%= link_to 'Edit', edit_catalogo_path(catalogo) %></td>
        <td><%= link_to 'Destroy', catalogo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Catalogo', new_catalogo_path %>
